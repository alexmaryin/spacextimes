<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_flow"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.MainFragment">
        <action
            android:id="@+id/action_showCrewMember"
            app:destination="@id/crewDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment" />
        <action
            android:id="@+id/action_showDragonDetails"
            app:destination="@id/dragonDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment" />
        <action
            android:id="@+id/action_showCoreDetails"
            app:destination="@id/coreDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment" />
        <action
            android:id="@+id/action_showRocketDetails"
            app:destination="@id/rocketDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"/>
        <action
            android:id="@+id/action_showLaunchPadDetails"
            app:destination="@id/launchPadDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"/>
        <action
            android:id="@+id/action_showLandingPadDetails"
            app:destination="@id/landingPadDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"/>
        <action
            android:id="@+id/action_showCapsuleDetails"
            app:destination="@id/capsuleDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"/>
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"/>
        <action
            android:id="@+id/action_showPreferences"
            app:destination="@id/preferencesFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/mainFragment"/>
    </fragment>
    <fragment
        android:id="@+id/crewDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.CrewDetailFragment">
        <argument
            android:name="crewId"
            app:argType="string" />
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/crewDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/dragonDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.DragonDetailFragment"
        tools:layout="@layout/dragon_detail_fragment">
        <argument
            android:name="dragonId"
            app:argType="string" />
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/dragonDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/coreDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.CoreDetailFragment" >
        <argument
            android:name="coreId"
            app:argType="string" />
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/coreDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/rocketDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.RocketDetailFragment"
        tools:layout="@layout/fragment_rocket_detail" >
        <argument
            android:name="rocketId"
            app:argType="string" />
    </fragment>
    <fragment
        android:id="@+id/launchPadDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.LaunchPadDetailFragment" >
        <argument
            android:name="launchPadId"
            app:argType="string" />
        <action
            android:id="@+id/action_showRocketDetails"
            app:destination="@id/rocketDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchPadDetailFragment"/>
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchPadDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/landingPadDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.LandingPadDetailFragment" >
        <argument
            android:name="landingPadId"
            app:argType="string" />
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/landingPadDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/capsuleDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.CapsuleDetailFragment" >
        <argument
            android:name="capsuleId"
            app:argType="string" />
        <action
            android:id="@+id/action_showLaunchDetails"
            app:destination="@id/launchDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/capsuleDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/launchDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.LaunchDetailFragment" >
        <argument
            android:name="launchId"
            app:argType="string" />
        <action
            android:id="@+id/action_showCoreDetails"
            app:destination="@id/coreDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchDetailFragment"/>
        <action
            android:id="@+id/action_showCrewDetails"
            app:destination="@id/crewDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchDetailFragment"/>
        <action
            android:id="@+id/action_showCapsuleDetails"
            app:destination="@id/capsuleDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchDetailFragment"/>
        <action
            android:id="@+id/action_showLaunchPadDetails"
            app:destination="@id/launchPadDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchDetailFragment"/>
        <action
            android:id="@+id/action_showPayloadDetails"
            app:destination="@id/payloadDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/launchDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/payloadDetailFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.PayloadDetailFragment">
        <argument
            android:name="payloadId"
            app:argType="string" />
        <action
            android:id="@+id/action_showCapsuleDetails"
            app:destination="@id/capsuleDetailFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim"
            app:popUpTo="@id/payloadDetailFragment"/>
    </fragment>
    <fragment
        android:id="@+id/preferencesFragment"
        android:name="ru.alexmaryin.spacextimes_rx.ui.view.fragments.PreferencesFragment"/>
</navigation>